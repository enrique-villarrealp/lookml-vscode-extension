{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "LookML",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#comment-line"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.lookml",
				"match": "\\b(if|while|for|return)\\b"
			},
		
			{
				"name": "keyword.view.lookml",
				"match": "\\b(view|dimension|dimension_group|measure|sql_table_name|hidden|description|sql|primary_key|type|timeframes|value_format|drill_fields|extends|extension|include|test|set|label|fields_hidden_by_default|suggestions|required_access_grants|cluster_keys|create_process|datagroup_trigger|derived_table|distribution|distribution_style|explore_source|increment_key|increment_offset|interval_trigger|materialized_view|indexes|partition_keys|persist_for|publish_as_db_view|sortkeys|sql|sql_create|sql_trigger_value|table_compression|table_format|final})\\b"
			},

			{
				"name": "keyword.view.table",
				"match": "\\b${TABLE}\\b"
			}
		
		]
		},
		"strings": {
			"name": "string.quoted.double.lookml",
			"begin": "\"",
			"beginCaptures": {
				"0": {"name": "punctuation.definition.string.begin.hx"} 
			},
			"end": "\"",
			"endCaptures": {
				"0": {"name": "punctuation.definition.string.begin.hx"} 
			},
			"patterns": [
				{
					"name": "constant.character.escape.lookml",
					"match": "\\\\."
				}
			]
		},

		"comment-line": {
			"name": "comment.line.block",
			"begin": "(//|#)",
			"end": "\n",
			"patterns": [{}]
		}
	},

	"scopeName": "source.lookml"
}